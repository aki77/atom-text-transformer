{
  "name": "text-transformer",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Transform selected text",
  "keywords": [
  ],
  "activationCommands": {
    "atom-text-editor": [
      "text-transformer:camelize",
      "text-transformer:capitalize",
      "text-transformer:dasherize",
      "text-transformer:uncamelcase",
      "text-transformer:underscore",
      "text-transformer:sequential"
    ]
  },
  "repository": "https://github.com/aki77/atom-text-transformer",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "underscore-plus": "~1.6.6",
    "atom-package-dependencies": "~1.0.3"
  },
  "consumedServices": {
    "sequential-command": {
      "versions": {
        "^0.1.0": "consumeSequentialCommand"
      }
    }
  },
  "package-dependencies": {
    "sequential-command": ">=0.3.0"
  }
}
